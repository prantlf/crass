{
  "name": "@prantlf/crass",
  "version": "0.12.5",
  "description": "A CSS utility library for JS",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "test": "mocha --recursive --reporter=dot",
    "grammar": "jison src/grammar.y --outfile=src/grammar.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/prantlf/crass.git"
  },
  "bin": {
    "crass": "./bin/crass"
  },
  "keywords": [
    "css3",
    "css",
    "parser",
    "minifier"
  ],
  "author": "Matt Basta",
  "license": "MIT",
  "dependencies": {
    "color-convert": "^2.0.1",
    "strong-data-uri": "^1.0.6",
    "@prantlf/svgo": "^0.7.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "browserify": "^16.5.2",
    "jison": "^0.4.18",
    "mocha": "^8.1.3"
  },
  "engines": {
    "node": ">=6.11.2"
  }
}
